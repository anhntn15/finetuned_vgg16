net: "../train_val.prototxt"
test_iter: 50
test_interval: 50
base_lr: 0.0001
lr_policy: "step"	# learning rate policy: drop the learning rate in "steps"
					# by a factor of gamma every stepsize iterations
gamma: 0.1
stepsize: 100	# drop the learning rate every 100K iterations
display: 50
max_iter: 1000
momentum: 0.9
weight_decay: 0.0005
snapshot: 50
snapshot_prefix: "../snapshot"
solver_mode: CPU
